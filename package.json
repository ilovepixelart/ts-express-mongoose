{
  "name": "ts-express-mongoose",
  "version": "1.0.0",
  "description": "Autogenerate REST api for your mongoose models",
  "author": "Alex Eagle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ilovepixelart/ts-express-mongoose.git"
  },
  "bugs": {
    "url": "https://github.com/ilovepixelart/ts-express-mongoose/issues"
  },
  "homepage": "https://github.com/ilovepixelart/ts-express-mongoosee#readme",
  "directories": {
    "examples": "examples"
  },
  "keywords": [
    "REST",
    "API",
    "express",
    "mongodb",
    "mongoose",
    "model",
    "backend",
    "mongo",
    "schema",
    "db",
    "nosql",
    "ts",
    "typescript",
    "swc"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist",
    "src",
    "tests",
    ".swcrc",
    "tsconfig.json",
    "jest.config.ts",
    "jest-mongodb-config.ts",
    ".eslintrc",
    ".eslintignore"
  ],
  "exports": {
    "import": "./dist/ts-express-mongoose",
    "require": "./dist/cjs/ts-express-mongoose"
  },
  "main": "./dist/cjs/ts-express-mongoose",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json && bash/esm.sh",
    "release": "npm install && npm run lint && npm run build && np"
  },
  "dependencies": {
    "dot-prop": "5.3.0",
    "serialize-error": "8.1.0",
    "lodash.isplainobject": "4.0.6"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.2.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.100",
    "@swc/helpers": "0.5.3",
    "@swc/jest": "0.2.29",
    "@swc/register": "0.1.10",
    "@types/cookie-parser": "1.4.6",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/lodash": "4.14.202",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "body-parser": "1.20.2",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "express": "4.18.2",
    "jest": "29.7.0",
    "merge": "2.1.1",
    "method-override": "3.0.0",
    "mongoose": "latest",
    "open-cli": "7.2.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "express": ">=4.0.0 < 5",
    "mongoose": ">=6.6.0 < 9"
  }
}